<h1>Log in</h1>

<div>
  <%= form_with(scope: :session, url: new_user_session_path) do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>

    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>

    <div>
      <%= form.submit "Log In" %>
    </div>
  <% end %>
</div>

<h1 class="text-3xl text-yellow-400 font-bold underline">
  Hello world!
</h1>